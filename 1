*******************************************************************************/

#include <stdio.h>

int main()
{
  int  row,col;
  printf("Enter Row : ");
  scanf("%d",&row);
  
  printf("Enter Col : ");
  scanf("%d",&col);
  
  
  for(int i=0;i<row;i++)
  {
      for(int j=0;j<col;j++)
      {
      printf("*");
      }
      printf("\n");
  }

    return 0;
}










