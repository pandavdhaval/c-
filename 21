
/******************************************************************************
A
AB
ABC
ABCD
ABCDE
ABCDEF

*******************************************************************************/

#include <stdio.h>
int main()
{
    
    int n;
    printf("enter numeber :");
    scanf("%d",&n);
    
    for(int i=1; i<=n; i++)
    {
        for(int j=1; j>n; j++)
        {
            printf(" ");
        }
        for(int k=1; k<=i; k++)
        {
            printf("%c",k+64);
        }
        
        printf("\n");
    }
    
    
    return 0;
}