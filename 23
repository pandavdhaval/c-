/******************************************************************************
1 2 3 4 5 6 
1         6 
1         6 
1         6 
1         6 
1 2 3 4 5 6 

*******************************************************************************/
#include <stdio.h>
int main()
{
    
    int n;
    printf("enter numeber :");
    scanf("%d",&n);
    
    for(int i=1; i<=n; i++ )
    {
        for(int j=1; j<=n; j++)
        {
            if(j==1||j==n||i==n||i==1)
            {
            printf("%d ",j);
            }
            else
            {
                printf("  ");
            }
        }
        printf("\n");
    }
    
    
    return 0;
}